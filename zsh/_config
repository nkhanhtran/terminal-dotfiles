ZSH=$HOME/.oh-my-zsh

plugins=(git command-not-found npm vagrant)
source $HOME/.nvm/nvm.sh
source $ZSH/oh-my-zsh.sh

export PATH="$HOME/.local/bin:/usr/lib/lightdm/lightdm:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games:/usr/local/games"

. $HOME/.local/lib/python2.7/site-packages/powerline/bindings/zsh/powerline.zsh

nvm use 0.10 1>/dev/null
